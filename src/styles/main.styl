@import 'prism/themes/prism-okaidia.css'
// Check out "bower_components/prism/themes/" for available themes
.slide1,.slide2
    width 100%
    height 100%
.s1,.s2,.s3,.s4,.s5,.s6
	background radial-gradient(orange,#ff3333)
.menu
    position absolute
    margin 0 auto
    img
        width 400px
        height 150px
.tombol
    width 100%
	margin 0 auto
	margin-top 50px
	a
        &:hover
            background-color orange
            color white
.tmbl1,.tmbl2,.tmbl3
    font-size 20px
    font-family "Lucida Sans Unicode", "Lucida Grande", sans-serif
    color black
    text-align center
    background-color #FC0
    border-color #FC0
    border-style outset
    text-decoration none
.tmbl1
    padding 5px 10px
.tmbl2
    padding 5px 11px
.tmbl3
    padding 5px 30px
.arr1
	float left
	
.arr2
	float right
.arr1,.arr2
	img
		width 70px
		height 70px
.slide3
	margin-top 60px
	text-align center
	font-family arial
	.judul1
		font-size 80px
		font-family NorthwoodHigh
		animation: color-change 1s infinite;
	li
		font-size 20px
		color black
	a
		margin-top 60px
@keyframes color-change {
    0% { color: red; }
    50% { color: blue; }
    100% { color: red; }
}
@font-face {
    font-family NorthwoodHigh
    src url("../font/Northwood_High.ttf")
}
.slide4
	text-align center
	margin-top 40px
	ul
		color black
		font-size 25px
	.l1
		color white
		font-size 40px
	a
		margin-top 100px
.slide5
	text-align left
	margin-top 20px
	ul
		color black
		font-size 25px
	.l2
		color white
		font-size 35px
	a
		margin-top 20px